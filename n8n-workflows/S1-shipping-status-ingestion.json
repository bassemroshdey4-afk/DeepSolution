          {
            "node": "Has Shipments?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Has Shipments?": {
      "main": [
        [
          {
            "node": "Split Shipments",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "No Shipments",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Split Shipments": {
      "main": [
        [
          {
            "node": "Call Carrier API",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Call Carrier API": {
      "main": [
        [
          {
            "node": "Transform Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Transform Response": {
      "main": [
        [
          {
            "node": "Ingest Status",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Ingest Status": {
      "main": [
        [
          {
            "node": "Success?",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Success?": {
      "main": [
        [
          {
            "node": "Success",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Log Dead Letter",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1",
    "saveManualExecutions": true,
    "callerPolicy": "workflowsFromSameOwner",
    "errorWorkflow": "error-handler"
