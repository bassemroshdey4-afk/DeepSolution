# Deep Solution Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# AI Provider Configuration
# ===========================================

# Active AI provider (gemini | openai | forge)
AI_PROVIDER=gemini

# Gemini API (uses Manus Forge by default)
# GEMINI_API_KEY=your-gemini-api-key
# GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta

# Default text model
AI_MODEL_TEXT=gemini-2.0-flash

# AI Safety Controls
ENABLE_AI_CALLS=true
ENABLE_AI_LOGGING=true
AI_MAX_TOKENS_PER_RUN=100000
AI_MAX_COST_PER_RUN=5

# ===========================================
# Database (Supabase)
# ===========================================

SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# ===========================================
# Authentication
# ===========================================

JWT_SECRET=your-jwt-secret
OAUTH_SERVER_URL=https://api.manus.im
VITE_APP_ID=your-app-id
VITE_OAUTH_PORTAL_URL=https://manus.im/oauth

# ===========================================
# Manus Forge API (Built-in)
# ===========================================

BUILT_IN_FORGE_API_URL=https://forge.manus.im
BUILT_IN_FORGE_API_KEY=your-forge-api-key
VITE_FRONTEND_FORGE_API_URL=https://forge.manus.im
VITE_FRONTEND_FORGE_API_KEY=your-frontend-forge-key

# ===========================================
# Application
# ===========================================

NODE_ENV=development
VITE_APP_TITLE=Deep Solution
VITE_APP_LOGO=/logo.svg
